{
    "manifest_version": 3,
    "name": "Beep Extension",
    "version": "1.0",
    "description": "This extension plays a beep sound.",
    "action": {
      "default_popup": "popup.html"
    },
    "web_accessible_resources": [
      {
        "resources": ["beep.mp3"],
        "matches": ["<all_urls>"]
      }
    ],
    "permissions": [
      "activeTab"
    ]
  }